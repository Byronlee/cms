= stylesheet_link_tag 'kr_newsflashes', media: 'all'
= render 'welcome/title', title: @newsflash.hash_title

.fast-section-detail-wrap
    .main-section
      .fast-section-detail.J_fastNewsDetailWrap
         = render partial: @newsflash.fast_type, locals: { newsflash: @newsflash }

      .index-side
        .fast-section.J_fastSection.mobile-hide
          = render 'welcome/aside'
